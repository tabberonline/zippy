  </Animated>\r\n      <Animated animationIn=\"slideInUp\" isVisible={true}>\r\n      <div id=\"achievements\">\r\n        <div className=\"mw1100 pl-40 flexColumn\">\r\n          <h1 className=\"achievements-heading\">Achievements we have made, so far</h1>\r\n          <div className=\"flexRow flexCenter\" style={{flexWrap: 'wrap'}}>\r\n              {Achievements.map(achievement => (\r\n                  <AchievementCard key={achievement.sr} name={achievement.name} desc={achievement.desc} image={achievement.sr === 1 ? users : links} />\r\n              ))}\r\n          </div>\r\n        </div>\r\n      </div>\r\n      </Animated>\r\n      <Animated animationIn=\"slideInUp\" isVisible={true}>\r\n      <div id=\"faq\">\r\n        <div className=\"mw1100 ph-40 flexColumn\">\r\n          <h1 className=\"tabberfeatures-heading\">Frequently Asked Questions</h1>\r\n          {QnA.map(ques => (\r\n              <FAQCard key={ques.sr} ques={ques.ques} ans={ques.ans} />\r\n          ))}\r\n        </div>\r\n       </div>\r\n      </Animated>\r\n      <Animated animationIn=\"slideInUp\" isVisible={true}>\r\n        <Footer />\r\n      </Animated>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default HomeScreen;\r\n","C:\\Users\\DELL\\Desktop\\Tabber Internship\\Tabber Main\\zippy\\src\\screens\\ContactScreen\\ContactScreen.js",[],"C:\\Users\\DELL\\Desktop\\Tabber Internship\\Tabber Main\\zippy\\src\\screens\\TermsScreen\\TermsScreen.js",[],"C:\\Users\\DELL\\Desktop\\Tabber Internship\\Tabber Main\\zippy\\src\\screens\\PolicyScreen\\PolicyScreen.js",[],"C:\\Users\\DELL\\Desktop\\Tabber Internship\\Tabber Main\\zippy\\src\\features\\user\\userSlice.js",[],"C:\\Users\\DELL\\Desktop\\Tabber Internship\\Tabber Main\\zippy\\src\\utility\\localStorageControl.js",[],"C:\\Users\\DELL\\Desktop\\Tabber Internship\\Tabber Main\\zippy\\src\\AdminServices\\baseUrl.js",[],"C:\\Users\\DELL\\Desktop\\Tabber Internship\\Tabber Main\\zippy\\src\\components\\Footer\\Footer.js",[],"C:\\Users\\DELL\\D